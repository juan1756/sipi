@model SIPI.Presentation.Website.Models.Cuenta.RecuperoMailModel
@{
    Layout = null;
    ViewBag.Title = "Recupero contraseña";
}

<table border="0" cellpadding="0" cellspacing="0" height="100%" width="100%" id="bodyTable">
    <tr>
        <td align="left" valign="top">
            <table style="font-family:'Open Sans', sans-serif;" border="0" cellpadding="10" cellspacing="0" width="600" id="emailContainer">
                <tr>
                    <td align="left" style="border-bottom: 1px solid #fe821d;" valign="top">
                        <table border="0" cellpadding="10" cellspacing="0" width="100%" id="emailHeader">
                            <tr>
                                <td align="left" valign="middle" style="width: 70px;">
                                    <img style="width: 60px;" src="data:image/png;base64," alt="logo" />
                                </td>
                                <td align="left" style="font-size: 30px; font-weight: bold;" valign="middle">
                                    Iglesia Calle Brasil
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td align="left" valign="top">
                        <table border="0" cellpadding="10" cellspacing="0" width="100%" id="emailBody">
                            <tr>
                                <td align="left" valign="top">
                                    <p>Hola @Model.Nombre @Model.Apellido:</p>

                                    <p>Hemos recibido una solicitud de <strong>cambio de contraseña</strong>.</p>

                                    <p>Si no has sido tú quien ha enviado la solicitud, ignora este mensaje. <br />Si  has sido tú, puedes restablecer la contraseña a través de este enlace:</p>

                                    <a style="color: #f9780e;" href="@Html.Raw(Url.Action("recuperoconfirmar", "cuenta", new { email = Model.Email, token = Model.Token, area = "" }, Request.Url.Scheme))">
                                        Confirmar cambio de contraseña
                                    </a>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td align="left" valign="top">
                        <table border="0" cellpadding="10" cellspacing="0" width="100%" id="emailFooter">
                            <tr>
                                <td align="left" valign="top">
                                    <p>
                                        Gracias, <br />
                                        Iglesia Calle Brasil.
                                    </p>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>