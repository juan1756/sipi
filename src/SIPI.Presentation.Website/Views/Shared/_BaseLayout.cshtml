<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shotcut icon" href="@Url.Content("~/content/assets/favicon.ico")" />

    <title>
        @(ViewBag.Title != null
                ? $"Iglesia Calle Brasil - {ViewBag.Title}"
                : "Iglesia Calle Brasil")
    </title>
    @Styles.Render("~/Content/styles")
</head>
<body>
        @RenderBody()

        @RenderSection("templates", required: false)

        @Scripts.Render("~/bundles/scripts/thirdparty")
        @Scripts.Render("~/bundles/scripts/plugins")
        @Scripts.Render("~/bundles/scripts/app")
        @RenderSection("scripts", required: false)
</body>
</html>
